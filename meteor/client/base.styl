@import url('https://fonts.googleapis.com/css?family=Open+Sans:400,700,300,300italic,400italic,700italic')

html, body
	font-family 'Open Sans', 'sans-serif'
	margin 0
	padding 0
	
h1, h2, h3, h4, h5, p, span, i, b, strong
	margin 0
	line-height 1.1em 
a, a:active, a:focus
	text-decoration none
	
object
	display none